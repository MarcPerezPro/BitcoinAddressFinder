      "staticAmount": 0,
      "useStaticAmount": true,
      "increaseMapAutomatically": true,
      "increaseSizeInMiB": 1,
      "lmdbDirectory": "lmdb",
      "useProxyOptimal": true,
      "logStatsOnInit": true,
      "logStatsOnClose": true,
      "loadToMemoryCacheOnInit" : false,
      "disableAddressLookup" : false
    }
  }
}