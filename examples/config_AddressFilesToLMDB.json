{
  "command": "AddressFilesToLMDB",
  "addressFilesToLMDB": {
    "addressesFiles": [
      "addresses/fileContainingAddresses0.txt",
      "addresses/fileContainingAddresses1.tsv",
      "addresses/fileContainingAddresses2.csv"
    ],
    "lmdbConfigurationWrite": {
      "initialMapSizeInMiB": 16,
